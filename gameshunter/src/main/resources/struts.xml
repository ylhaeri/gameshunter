<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
  "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
  "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<constant name="struts.custom.i18n.resources" value="messages"></constant>

	<package name="GHCore" extends="struts-default">
		<interceptors>

			<interceptor name="persistence"
				class="br.com.gameshunter.interceptor.PersistenceInterceptor" />

			<interceptor-stack name="GHDefault">
				<interceptor-ref name="persistence" />
				<interceptor-ref name="defaultStack" />
			</interceptor-stack>

		</interceptors>
	</package>
</struts>